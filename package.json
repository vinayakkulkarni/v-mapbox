    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/component-compiler-utils": "^3.3.0",
