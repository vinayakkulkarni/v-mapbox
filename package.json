    "require-extension-hooks-vue": "^3.0.0",
    "rollup": "^2.33.1",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "shipjs": "^0.24.0",
    "typescript": "^4.0.5",
    "vue": "^3.2.23",
    "vuepress": "^2.0.0-beta.27"
  },
  "engines": {
    "node": ">=12.18.3"
