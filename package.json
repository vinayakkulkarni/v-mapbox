    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-vue": "^2.0.2",
    "browser-env": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
